		android:layout_width="324.0dp"
		android:layout_height="296.5dp"
		android:layout_toRightOf="@id/linearLayout1"
		android:id="@+id/note_container"
		android:layout_marginTop="63.0dp"
		android:layout_marginLeft="0.0dp" />
</RelativeLayout>