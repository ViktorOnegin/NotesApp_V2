		layout="@layout/toolbar"
		android:layout_height="522.0dp" />
	<LinearLayout
		android:orientation="vertical"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="315.0dp"
		android:layout_height="552.5dp"
		android:layout_below="@id/toolbar"
		android:id="@+id/linearLayout1">
		<fragment
			android:name="Note_V2.TitlesFragment"
			android:layout_width="318.0dp"
			android:layout_height="295.5dp"
			android:id="@+id/fragment1" />
	</LinearLayout>
	<FrameLayout
		android:minWidth="25px"
